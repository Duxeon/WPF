<Window x:Class="WPF_mathiasS_Act4.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_mathiasS_Act4"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="800">
    <Grid Background="#FFEEBB33" >
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="1*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="2*"/>
            <RowDefinition Height="3*"/>
        </Grid.RowDefinitions>
        <TextBlock Text="Exemple Multipage" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" FontSize="40"/>
        <Button x:Name="bp1" Grid.Column="1" Height="96" VerticalAlignment="Top" Background="Red" Foreground="White" FontSize="60" Content="Page 1"/>
        <Button x:Name="bp2" Grid.Column="1" Height="96" VerticalAlignment="Bottom" Background="Blue" Foreground="White" FontSize="60" Content="Page 2"/>
        <Frame x:Name="main" NavigationUIVisibility="Hidden" Grid.ColumnSpan="2" Grid.Row="1"/>
        
    </Grid>
</Window>
